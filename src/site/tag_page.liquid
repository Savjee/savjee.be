---
layout: page
eleventyComputed:
  title: Tagged "{{ tag }}"
pagination:
  data: collections
  size: 1
  alias: tag
permalink: /tag/{{ tag | slugify }}/
---

<ol>
  {% assign taglist = collections[tag] | sort: "date" | reverse %}
  {% for post in taglist | reverse %}
    <li>
      <a href="{{ post.url }}">{{ post.data.title }}</a>
      {{ post.date }}
      {% if post.url | startsWith: "/blog" %}
        post!
      {% endif %}
    </li>
  {% endfor %}
</ol>